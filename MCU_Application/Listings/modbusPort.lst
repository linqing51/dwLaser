   5          void initModbusSerial(int32_t baudrate)
   6          {//濮MODBUS涓插
   7   1      #ifdef C8051F020
   8   1              CKCON |= (1 << 5);//Timer2 uses the system clock
   9   1              T2CON &= ~(1 << 0);//褰瀹跺2 婧㈠烘T2EX 涓璐璺冲跺ㄩ瑁杞斤EXEN2=1锛
  10   1              T2CON &= ~(1 << 1);//瀹跺ㄥ斤瀹跺2 T2M锛CKCON.5锛瀹涔堕1
  11   1              T2CON |= (1 << 4);//Timer 2 overflows used for transmit clock.
  12   1              T2CON |= (1 << 5);//Timer 2 overflows used for receive clock.   
  13   1              RCAP2  = - ((long) ((uint32_t)CONFIG_SYSCLK / baudrate) / 32L);
  14   1              TMR2 = RCAP2;
